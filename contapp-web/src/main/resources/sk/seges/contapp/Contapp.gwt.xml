<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.0-ms1/distro-source/core/src/gwt-module.dtd">
<module rename-to="contapp">
        <inherits name='sk.seges.contapp.Mobile'/>
        <inherits name='sk.seges.contapp.Desktop'/>
                	
        <source path="client"/>
        
		<set-property name="user.agent" value="safari" />

		<add-linker name="xsiframe" />
	
		<inherits name="com.google.gwt.logging.Logging"/>
		<set-property name="gwt.logging.simpleRemoteHandler" value="DISABLED" />
		<set-property name="gwt.logging.popupHandler" value="DISABLED" />
		
		<set-configuration-property name="devModeRedirectEnabled" value="true"/>
		<set-configuration-property name="mgwt.superdevmode" value="on" />
		
		<set-property name="compiler.emulatedStack" value="true"/>
		<set-configuration-property name="compiler.emulatedStack.recordLineNumbers" value="true"/>
		<set-configuration-property name="compiler.emulatedStack.recordFileNames" value="true"/>
	
		<define-configuration-property name="CssResource.style" is-multi-valued="false" />
        <set-configuration-property name="CssResource.style" value="pretty" />
		    	
		<entry-point class="sk.seges.contapp.client.ContappEntryPoint" />
</module>